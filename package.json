    "minimist": "^1.2.5",
    "rc": "^1.2.8",
    "semver": "^7.3.4",
    "string.prototype.replaceall": "^1.0.4"
  },
  "devDependencies": {
    "c8": "^7.6.0",
    "eslint": "^7.20.0",
    "eslint-config-groupon": "^10.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
